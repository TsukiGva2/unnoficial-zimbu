IMPORT testdir/Common.zu       # tests shared with jstestprog.zu
IMPORT testdir/ExitTest.zu

FUNC Main() int

  IO.print("Running JS tests")

  Common.test()

  ExitTest.exit()

  IO.print("Not reached")

  RETURN 0
}
