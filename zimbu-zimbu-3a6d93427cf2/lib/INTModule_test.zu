FUNC test_ForSkip() status
  list<nat> l = [0, 1, 2, 3, 4]
  list<nat> r = []
  FOR idx IN 0 UNTIL l.Size()
    r.add(l[idx])
    IF idx == 2
      idx++
    }
  }
  TEST.equal([0, 1, 2, 4], r)
  RETURN OK
}
