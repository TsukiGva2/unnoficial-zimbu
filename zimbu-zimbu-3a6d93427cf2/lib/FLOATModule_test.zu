FUNC test_toInt() status
  TEST.equal(0, 0.1.toInt())
  TEST.equal(1, 0.5.toInt())
  TEST.equal(-1, -0.5.toInt())
  TEST.equal(INT.max, INF.toInt())
  TEST.equal(INT.min, NINF.toInt())
  TEST.equal(0, NAN.toInt())

  RETURN OK
}
