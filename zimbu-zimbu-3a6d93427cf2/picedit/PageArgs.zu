CLASS PageArgs
  string $imageDir
  string $imageName

  FUNC $setImageName(string imageName) PageArgs
    $imageName = imageName
    RETURN THIS
  }

  FUNC $setImageDir(string imageDir) PageArgs
    $imageDir = imageDir
    RETURN THIS
  }
}
